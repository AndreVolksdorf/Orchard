@model SmartPage.LikeButton.Settings.LikeButtonPartSettings

<fieldset>
    <div class="editor-label">
        @Html.LabelFor(model => model.Dimension, T("Dimension"))
    </div>
    <div class="editor-field">
        @Html.TextBoxFor(model => model.Dimension)
        @Html.Hint(T("The dimension you "))
        @Html.ValidationMessageFor(model => model.Dimension)
    </div>
    <div class="editor-label">
        @Html.LabelFor(model => model.LikeButtonText, T("LikeButton text"))
    </div>
    <div class="editor-field">
        @Html.TextBoxFor(model => model.LikeButtonText)
        @Html.ValidationMessageFor(model => model.LikeButtonText)
    </div>
    <div class="editor-label">
        @Html.LabelFor(model => model.UnlikeButtonText, T("UnlikeButton text"))
    </div>
    <div class="editor-field">
        @Html.TextBoxFor(model => model.UnlikeButtonText)
        @Html.ValidationMessageFor(model => model.UnlikeButtonText)
    </div>
    <div>
        @Html.EditorFor(m => m.ShowVoter)
        <label class="forcheckbox" for="@Html.FieldIdFor(m => m.ShowVoter)">@T("Enable favorite marker")</label>
    </div>
</fieldset>